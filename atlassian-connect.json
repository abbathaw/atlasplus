{
  "key": "atlasplus",
  "name": "Atlas Plus Video Studio",
  "description": "The ultimate video solution for all of your needs",
  "baseUrl": "{{localBaseUrl}}",
  "links": {
    "self": "{{localBaseUrl}}/atlassian-connect.json",
    "homepage": "{{localBaseUrl}}/atlassian-connect.json"
  },
  "authentication": {
    "type": "jwt"
  },
  "lifecycle": {
    "installed": "/installed",
    "uninstalled": "/uninstalled"
  },
  "scopes": ["READ", "WRITE"],
  "modules": {
    "generalPages": [
      {
        "key": "videoStudio",
        "location": "",
        "name": {
          "value": "Video Studio"
        },
        "url": "/video-studio?spaceKey={space.key}",
        "conditions": [
          {
            "condition": "user_is_logged_in"
          }
        ]
      }
    ],
    "customContent": [
      {
        "key": "studio",
        "name": {
          "value": "Video Studio"
        },
        "apiSupport": {
          "supportedContainerTypes": ["space"]
        },
        "uiSupport": {
          "listViewComponent": {
            "moduleKey": "videoStudio"
          },
          "contentViewComponent": {
            "moduleKey": "videoStudio"
          },
          "icons": {
            "item": {
              "url": "images/logo.png"
            }
          }
        }
      }
    ],
    "dynamicContentMacros": [
      {
        "key": "video-macro",
        "name": {
          "value": "Video Macro"
        },
        "url": "/video-macro",
        "description": {
          "value": "Embed a video uploaded through the Video Studio"
        },
        "icon": {
          "width": 80,
          "height": 80,
          "url": "images/logo.png"
        },
        "outputType": "block",
        "bodyType": "none"
      }
    ],
    "adminPages": [
      {
        "url": "/get-started",
        "key": "get-started-admin-page",
        "location": "system.admin/atlasplus-plugin-admin-menu-section",
        "name": {
          "value": "Get Started"
        }
      },
      {
        "url": "/configuration",
        "key": "configuration-admin-page",
        "location": "system.admin/atlasplus-plugin-admin-menu-section",
        "weight": 105,
        "name": {
          "value": "Configuration"
        }
      }
    ],
    "webSections": [
      {
        "key": "atlasplus-plugin-admin-menu-section",
        "location": "system.admin",
        "weight": 100,
        "name": {
          "value": "Atlas+"
        }
      }
    ],
    "postInstallPage": {
      "url": "/get-started",
      "key": "get-started-post-install",
      "name": {
        "value": "Get Started: Atlas+ Video Studio"
      }
    },
    "configurePage": {
      "url": "/configuration",
      "key": "config-page-post-install",
      "name": {
        "value": "Atlas+ Configuration"
      }
    }
  }
}
