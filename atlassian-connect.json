{
  "key": "atlasplus-staging",
  "name": "Atlas Plus Video Studio",
  "description": "The ultimate video solution for all of your needs",
  "baseUrl": "{{localBaseUrl}}/",
  "links": {
    "self": "{{localBaseUrl}}/atlassian-connect.json",
    "homepage": "{{localBaseUrl}}/atlassian-connect.json"
  },
  "authentication": {
    "type": "jwt"
  },
  "lifecycle": {
    "installed": "/installed",
    "uninstalled": "/uninstalled"
  },
  "scopes": ["READ", "WRITE"],
  "modules": {
    "generalPages": [
      {
        "key": "videoStudio",
        "location": "",
        "name": {
          "value": "Video Studio"
        },
        "url": "/video-studio?spaceKey={space.key}",
        "conditions": [
          {
            "condition": "user_is_logged_in"
          }
        ]
      }
    ],
    "customContent": [
      {
        "key": "studio",
        "name": {
          "value": "Video Studio"
        },
        "apiSupport": {
          "supportedContainerTypes": ["space"]
        },
        "uiSupport": {
          "listViewComponent": {
            "moduleKey": "videoStudio"
          },
          "contentViewComponent": {
            "moduleKey": "videoStudio"
          },
          "icons": {
            "item": {
              "url": "images/logo.png"
            }
          }
        }
      }
    ],
    "dynamicContentMacros": [
      {
        "key": "video-macro",
        "name": {
          "value": "Video"
        },
        "url": "/video-macro?macroId={macro.id}",
        "description": {
          "value": "Embed a video uploaded through the Video Studio."
        },
        "icon": {
          "width": 80,
          "height": 80,
          "url": "images/logo.png"
        },
        "outputType": "block",
        "bodyType": "none",
        "categories": ["media", "confluence-content", "external-content"],
        "parameters": [
          {
            "identifier": "users",
            "name": {
              "value": "Assigned User(s)"
            },
            "type": "username",
            "required": false,
            "multiple": true
          },
          {
            "identifier": "video",
            "name": {
              "value": "Video"
            },
            "type": "enum",
            "required": true,
            "multiple": false
          }
        ],
        "editor": {
          "url": "/editor",
          "editTitle": {
            "value": "Edit 'Video' Macro"
          },
          "insertTitle": {
            "value": "Insert 'Video' Macro"
          },
          "width": "650px",
          "height": "400px"
        }
      }
    ],
    "adminPages": [
      {
        "url": "/get-started",
        "key": "get-started-admin-page",
        "location": "system.admin/atlasplus-plugin-admin-menu-section",
        "name": {
          "value": "Get Started"
        }
      }
    ],
    "webSections": [
      {
        "key": "atlasplus-plugin-admin-menu-section",
        "location": "system.admin",
        "weight": 100,
        "name": {
          "value": "Atlas+: Video Solutions"
        }
      }
    ],
    "postInstallPage": {
      "url": "/get-started",
      "key": "get-started-post-install",
      "name": {
        "value": "Get Started: Atlas+ Video Studio"
      }
    }
  }
}
