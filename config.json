{
  "development": {
    "port": 3000,
    "errorTemplate": true,
    "store": {
      "adapter": "sequelize",
      "dialect": "postgres",
      "url": "$POSTGRES_URL",
      "username": "$DBUSERNAME",
      "password": "$DBPASSWORD",
      "database": "$DBNAME",
      "logging": false
    }
  },
  "production": {
    "port": "$PORT",
    "errorTemplate": true,
    "localBaseUrl": "https://my-subdomain.herokuapp.com",
    "store": {
      "adapter": "sequelize",
      "dialect": "postgres",
      "url": "$POSTGRES_URL",
      "username": "$DBUSERNAME",
      "password": "$DBPASSWORD",
      "database": "$DBNAME"
    },
    "whitelist": [
      "*.jira-dev.com",
      "*.atlassian.net",
      "*.atlassian.com",
      "*.jira.com"
    ]
  },
  "product": "confluence"
}
